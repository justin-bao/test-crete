<crete>
    <mode>developer</mode>
    <vm>
      <arch>x86</arch>
    </vm>
    <svm>
        <args>
            <symbolic>
              --search=dfs
              --randomize-fork=false
              --check-overshift=false
              --use-cex-cache
              --use-forked-solver
              --simplify-sym-indices
              --max-time=300.
              --max-instruction-time=5.
	      --max-memory=2000
            </symbolic>
        </args>
    </svm>
    <test>
        <interval>
            <trace>10000</trace>
            <!-- <tc>30</tc> -->
            <!-- <time>900</time> -->
        </interval>
    </test>
</crete>
